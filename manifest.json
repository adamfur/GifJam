{
  "name": "Gif Jam (Animation Stopper)",
  "short_name": "Gif Jam",
  "version": "1.0.0.13",
  "description": "Stop Gif Animations",
  "permissions": ["tabs", "webRequest", "webRequestBlocking", "http://*/*.gif", "https://*/*.gif", "http://*/*.gif?*", "https://*/*.gif?*", "http://*/*.gif&*", "https://*/*.gif&*"],
  "icons": {
    "128": "logo128.png",
    "64": "logo64.png",
    "32": "logo32.png"
  },
  "browser_action": {
    "default_title": "Gif Jam"
  },  
  "background": {
    "scripts": ["states.js", "parser.js", "background.js"]
  },
  "manifest_version": 2
}
